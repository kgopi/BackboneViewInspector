{
  "manifest_version": 2,
  "name": "Backbone View Tracker",
  "description": "This extension will track the backbone views",
  "version": "0.1",
  "browser_action": {
    "default_icon": {
      "19": "images/19/inactive.png",
      "38": "images/38/inactive.png"
    },
    "default_title": "Track Backbone views powering the page"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "css": ["css/app.css"],
      "js": ["js/lib/jquery-1.5.2.min.js", "js/app.js"],
      "run_at": "document_start",
      "all_frames":true
    }
  ],
  "background": { "scripts": ["js/background.js"] },
  "web_accessible_resources": [
    "js/*",
    "template/*"
  ]/*,
  "icons": {
    "16":	"icons/icon16.png",
    "48":	"icons/icon48.png",
    "128":	"icons/icon128.png"
  }*/
}
